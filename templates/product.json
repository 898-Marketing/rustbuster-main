            "heading_tag": "h2"
          }
        },
        "text_jLUiz4": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.custom.product_dimensions_text | metafield_tag }}",
            "text_size": "t3"
          }
        },
        "custom_liquid_jrRjjE": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% comment %}\n  Display product dimensions image from metafield with lightbox functionality\n  Metafield: custom.product_dimensions_image\n{% endcomment %}\n\n{% if product.metafields.custom.product_dimensions_image %}\n  <div class=\"product-dimensions-wrapper\" style=\"margin: 20px 0;\">\n    \n    {%- assign dimensions_image = product.metafields.custom.product_dimensions_image -%}\n    \n    <a href=\"{{ dimensions_image | image_url }}\" \n       class=\"dimensions-image-link\" \n       data-lightbox=\"product-dimensions\"\n       style=\"display: inline-block; cursor: zoom-in;\">\n      <img src=\"{{ dimensions_image | image_url: width: 800 }}\" \n           alt=\"Product Dimensions for {{ product.title }}\"\n           loading=\"lazy\"\n           style=\"max-width: 100%; height: auto; border: 1px solid #e0e0e0; border-radius: 4px;\">\n    </a>\n    <p style=\"font-size: 0.875rem; color: #666; margin-top: 8px;\">\n      Click image to view full size\n    </p>\n  </div>\n\n  <style>\n    .dimensions-image-link:hover img {\n      opacity: 0.9;\n      transition: opacity 0.2s ease;\n    }\n    \n    /* Lightbox overlay */\n    .lightbox-overlay {\n      display: none;\n      position: fixed;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      background: rgba(0, 0, 0, 0.9);\n      z-index: 9999;\n      justify-content: center;\n      align-items: center;\n      cursor: zoom-out;\n    }\n    \n    .lightbox-overlay.active {\n      display: flex;\n    }\n    \n    .lightbox-image {\n      max-width: 95%;\n      max-height: 95%;\n      object-fit: contain;\n    }\n    \n    .lightbox-close {\n      position: absolute;\n      top: 20px;\n      right: 30px;\n      font-size: 40px;\n      color: white;\n      cursor: pointer;\n      z-index: 10000;\n    }\n  </style>\n\n  <script>\n    document.addEventListener('DOMContentLoaded', function() {\n      const links = document.querySelectorAll('[data-lightbox=\"product-dimensions\"]');\n      \n      links.forEach(link => {\n        link.addEventListener('click', function(e) {\n          e.preventDefault();\n          \n          // Create lightbox if it doesn't exist\n          let lightbox = document.querySelector('.lightbox-overlay');\n          if (!lightbox) {\n            lightbox = document.createElement('div');\n            lightbox.className = 'lightbox-overlay';\n            \n            const closeBtn = document.createElement('span');\n            closeBtn.className = 'lightbox-close';\n            closeBtn.innerHTML = '&times;';\n            \n            const img = document.createElement('img');\n            img.className = 'lightbox-image';\n            \n            lightbox.appendChild(closeBtn);\n            lightbox.appendChild(img);\n            document.body.appendChild(lightbox);\n            \n            // Close on click\n            lightbox.addEventListener('click', function() {\n              lightbox.classList.remove('active');\n            });\n          }\n          \n          // Set image and show lightbox\n          const imgElement = lightbox.querySelector('.lightbox-image');\n          imgElement.src = this.href;\n          lightbox.classList.add('active');\n        });\n      });\n      \n      // Close on Escape key\n      document.addEventListener('keydown', function(e) {\n        if (e.key === 'Escape') {\n          const lightbox = document.querySelector('.lightbox-overlay');\n          if (lightbox) {\n            lightbox.classList.remove('active');\n          }\n        }\n      });\n    });\n  </script>\n{% endif %}"
          }
        },
